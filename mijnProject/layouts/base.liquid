<html lang="en">
<head>
  {% block head %}
    <title>My First Page</title>
  {% endblock %}
  {% if NODE_ENV == "development" %}
    <script type="module" src="http://localhost:5173/@vite/client" defer></script>
    <script type="module" src="http://localhost:5173/src/index.js" defer></script>
  {% else %}
  <link rel="stylesheet" href="./styles/main.css">
  {% comment %} <script type="module" src="/main.js" defer></script> {% endcomment %}
  <script type="nomodule" src="/polyfills-legacy.js" defer></script>
  <script type="nomodule" src="/main-legacy.js" defer></script>
  {% endif %}
  {% block styles %}{% endblock %}
  {% block scripts %}{% endblock %}
</head>

<body>
  {% block content %}{% endblock %}
  <script src="./scripts/script.js"></script>
</body>
</html>
